        {
            "Info": {
                "SdfMetadata": {
                    "arnold:filename": {
                        "appliesTo": [
                            "prims"
                        ],
                        "default": "<built-in>",
                        "displayGroup": "Arnold",
                        "documentation": "Arnold plugin location for a node.",
                        "type": "token"
                    }
                },
                "Types": {
                    "NdrArnoldParserPlugin" : {
                        "bases": ["${REGISTRY_BASE}ParserPlugin"],
                        "displayName": "Arnold Node Parser"
                    },
                    "NodeRegistryArnoldDiscoveryPlugin" : {
                        "bases": ["${REGISTRY_BASE}DiscoveryPlugin"],
                        "displayName": "Arnold Node Discovery"
                    }
                }
            },
            "LibraryPath": "${LIB_PATH}${LIB_EXTENSION}",
            "Name": "nodeRegistryArnold",
            "ResourcePath": "resources",
            "Root": "..",
            "Type": "library"
        }
    ]
}
