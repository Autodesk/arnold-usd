#usda 1.0
(
    defaultPrim = "cube"
)


def Cylinder "cylinder"
{
    double radius=1.1
    double height=3
    token axis="Y"
	float3 xformOp:translate= (0,0, 0)
    uniform token[] xformOpOrder = ["xformOp:translate"]
}

def Cone "cone"
{
    double radius=1.6
    double height=2.7
    token axis="X"
	float3 xformOp:translate= (0,5, 0)
    uniform token[] xformOpOrder = ["xformOp:translate"]
}
