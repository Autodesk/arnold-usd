#usda 1.0
(
    defaultPrim = "cube"
)

def Cylinder "cylinder" (
    apiSchemas = ["MaterialBindingAPI"]
)
{
    token axis = "Y"
    double height = 3
    double radius = 1.1
    float3 xformOp:translate = (0, 0, 0)
    uniform token[] xformOpOrder = ["xformOp:translate"]
}

def Cone "cone" (
    apiSchemas = ["MaterialBindingAPI"]
)
{
    token axis = "X"
    double height = 2.7
    double radius = 1.6
    float3 xformOp:translate = (0, 5, 0)
    uniform token[] xformOpOrder = ["xformOp:translate"]
}

