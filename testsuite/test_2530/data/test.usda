def Scope "mtl"
{
    def Material "flat"
    {
        token outputs:arnold:surface.connect = </mtl/flat/flat1.outputs:out>

        def Shader "flat1"
        {
            uniform token info:id = "arnold:flat"
            color3f inputs:color.connect = </mtl/flat/userData.outputs:out>
            token outputs:out
        }
        def Shader "userData"
        {
            uniform token info:id = "arnold:user_data_rgb"
            string inputs:attribute = "test_color"
            token outputs:out
        }
    }
}
